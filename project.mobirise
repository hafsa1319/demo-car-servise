          "_name": "footer01",
          "_customHTML": "<section class=\"footer01\" group=\"Footer\" global=\"\" plugins=\"\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <select title=\"Align\" name=\"itemsAlign\">\n            <option value=\"flex-start\" selected=\"\">Left</option>\n            <option value=\"center\">Center</option>\n            <option value=\"flex-end\">Right</option>\n        </select>\n\n        <input type=\"range\" inline=\"\" name=\"list1Items\" title=\"Items 1\" min=\"0\" max=\"5\" value=\"3\">\n        <input type=\"range\" inline=\"\" name=\"items1\" title=\"Items 2\" min=\"0\" max=\"5\" value=\"4\">\n        <input type=\"range\" inline=\"\" name=\"items2\" title=\"Items 3\" min=\"0\" max=\"5\" value=\"4\">\n        <input type=\"range\" inline=\"\" title=\"Hours Items\" name=\"hours\" min=\"1\" max=\"8\" step=\"1\" value=\"3\">\n\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"List 1 Icons\" name=\"listicon\" min=\"0.2\" max=\"2\" step=\"0.1\" value=\"1.1\">\n        <input type=\"range\" inline=\"\" title=\"Check Icons\" name=\"checkIconSize\" min=\"0.2\" max=\"2\" step=\"0.1\" value=\"1.1\">\n        <header>Colors</header>\n        <input type=\"color\" title=\"Hover\" name=\"hoverColor\" value=\"#ff4e3c\" selected=\"\">\n        <input type=\"color\" title=\"Days\" name=\"daysColor\" value=\"#ffffff\" selected=\"\">\n        <input type=\"color\" title=\"Times\" name=\"timesColor\" value=\"#ffffff\" selected=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#06182d\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#06182d\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div class=\"row align-left\">\n            <div class=\"col-md-6 col-lg-3\">\n                <h3 class=\"title pb-2 align-left mbr-bold mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title\">Carm4</h3>\n                <div class=\"items mbr-white\" mbr-list=\"\" mbr-list-grow=\"list1Items\">\n                    <div class=\"list-item\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont listico fa-map-pin fa\" style=\"color: rgb(255, 78, 60);\"></span>\n                        <h5 class=\"mbr-fonts-style text2\" mbr-theme-style=\"display-4\" data-app-selector=\".items .list-item .text2, .items .list-item\">0111\n                            Example Avenue NoCity, Alaska 11234</h5>\n                    </div>\n                    <div class=\"list-item\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont listico mdi-communication-call\" style=\"color: rgb(255, 78, 60);\"></span>\n                        <h5 class=\"mbr-fonts-style text2\" mbr-theme-style=\"display-4\" data-app-selector=\".items .list-item .text2, .items .list-item\">\n                            +0 987 65 431</h5>\n                    </div>\n                    <div class=\"list-item\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont listico mdi-communication-email\" style=\"color: rgb(255, 78, 60);\"></span>\n                        <h5 class=\"mbr-fonts-style text2\" mbr-theme-style=\"display-4\" data-app-selector=\".items .list-item .text2, .items .list-item\">\n                            example@info.top</h5>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-md-6 col-lg-3\">\n                <h2 class=\"title mbr-bold mbr-white pb-2 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".title\">\n                    Company\n                </h2>\n\n                <div class=\"align-wrap\" mbr-if=\"items1 > 0\">\n\n                    <div class=\"item-wrap\">\n                        <div class=\"card-img\"><span mbr-icon=\"\" class=\"mbr-iconfont img2 fa-check-circle-o fa\"></span>\n                        </div>\n                        <div class=\"icons-wrap pb-2\">\n                            <p class=\"icon-title mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".align-wrap .item-wrap .icons-wrap .icon-title, .align-wrap\">\n                                About Us<br></p>\n                        </div>\n                    </div>\n\n                    <div class=\"item-wrap\" mbr-if=\"items1 > 1\">\n                        <div class=\"card-img\"><span mbr-icon=\"\" class=\"mbr-iconfont img2 fa-check-circle-o fa\"></span>\n                        </div>\n                        <div class=\"icons-wrap pb-2\">\n                            <p class=\"icon-title mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".align-wrap .item-wrap .icons-wrap .icon-title, .align-wrap\">Contact Us</p>\n                        </div>\n                    </div>\n\n                    <div class=\"item-wrap\" mbr-if=\"items1 > 2\">\n                        <div class=\"card-img\"><span mbr-icon=\"\" class=\"mbr-iconfont img2 fa-check-circle-o fa\"></span>\n                        </div>\n      